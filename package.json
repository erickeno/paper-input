{
  "name": "paper-input",
  "version": "1.1.0",
  "description": "Paper Input React component",
  "main": "dist/index.js",
  "scripts": {
    "prepublish": "rm -rf dist; mkdir dist; npm run build",
    "build": "npm run build-js && npm run build-css",
    "build-js": "NODE_ENV=production babel src --out-dir dist",
    "build-css": "postcss --use postcss-cssnext -o dist/paper-input.css src/index.css",
    "watch": "npm run watch-js & npm run watch-css",
    "watch-js": "watchify ./example/index.js -t [ babelify --presets [ es2015 react ] ] -o ./example/bundle.js -dv",
    "watch-css": "nodemon -e css -w ./src/ --exec 'npm run build-css'",
    "test": "mocha --compilers js:babel-register"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/scienceai/paper-input.git"
  },
  "keywords": [
    "Material UI",
    "Material Design Lite",
    "react",
    "Paper",
    "scientific UI"
  ],
  "author": "Chris Callahan <chris@standardanalytics.io>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/scienceai/paper-input/issues"
  },
  "homepage": "https://github.com/scienceai/paper-input",
  "dependencies": {
    "classnames": "2.2.3"
  },
  "peerDependencies": {
    "react": ">= 0.14.0",
    "react-dom": ">= 0.14.0"
  },
  "devDependencies": {
    "babel-cli": "6.7.5",
    "babel-eslint": "6.0.2",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-register": "6.7.2",
    "babelify": "^7.2.0",
    "eslint": "2.8.0",
    "eslint-plugin-react": "5.0.1",
    "mocha": "2.4.5",
    "nodemon": "1.9.1",
    "postcss": "5.0.19",
    "postcss-cli": "2.5.1",
    "postcss-cssnext": "2.5.2",
    "react": "15.0.1",
    "react-addons-test-utils": "15.0.1",
    "react-dom": "15.0.1",
    "watchify": "3.7.0"
  }
}
