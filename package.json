  },
  "homepage": "https://github.com/standard-analytics/paper-input",
  "dependencies": {
    "react": ">=0.13.0 || 0.14.0-rc1"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "cssnext": "^1.8.4"
  }
}
