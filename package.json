    "classnames": "^2.1.3",
    "react": "^0.14.0-rc1"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "cssnext": "^1.8.4",
    "mocha": "^2.3.2",
    "react-addons-test-utils": "^0.14.0-rc1"
  }
